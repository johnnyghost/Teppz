{
	"name": "eppz! Code",
	"include": "eppz-code-regions-using.json",
	"settings": [
		{
			"name": "Source",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#444"
			}
		},
		{
			"name": "Keyword (operator)",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Punctuation (parenthesis)",
			"scope": [
				"punctuation.parenthesis",
				"meta.brace.round"
			],
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Punctuation (square bracket)",
			"scope": [
				"punctuation.squarebracket",
				"meta.brace.square"
			],
			"settings": {
				"foreground": "#444"
			}
		},
		{
			"name": "Punctuation (curly brace)",
			"scope": [
				"punctuation.curlybrace"
			],
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"comment punctuation",
				"comment punctuation.parenthesis",
				"comment punctuation.squarebracket",
				"comment punctuation.curlybrace",
				"comment keyword",
				"comment entity",
				"comment variable",
				"comment string"
			],
			"settings": {
				"foreground": "#333",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Literals (Constant)",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#5064f2"
			}
		},
		{
			"name": "Literals (null)",
			"scope": [
				"constant.language.null"
			],
			"settings": {
				"foreground": "#48548a"
			}
		},
		{
			"name": "Literals (Number)",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#508af2"
			}
		},
		{
			"name": "Literals (String)",
			"scope": [
				"string.quoted.double",
				"string.quoted.double punctuation"
			],
			"settings": {
				"foreground": "#10a0f2",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Literals (Escape)",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#386e7a"
			}
		},
		{
			"name": "Literals (Char)",
			"scope": [
				"string.quoted.single",
				"string.quoted.single punctuation"
			],
			"settings": {
				"foreground": "#1b5f53"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"entity.name.method",
				"keyword.other.get",
				"keyword.other.set",
				"keyword.other.add",
				"keyword.other.remove",
				"support.function",
				"keyword.operator.new"
			],
			"settings": {
				"foreground": "#1d5566"
			}
		},
		{
			"name": "Functions (System)",
			"scope": [
				"support.function.System"
			],
			"settings": {
				"foreground": "#203631"
			}
		},
		{
			"name": "Types",
			"comment": "Including classes, delegates, enums, interfaces, structs.",
			"scope": [
				"storage.type",
				"entity.name.type.type-parameter",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#a6574b"
			}
		},
		{
			"name": "Types (built-in)",
			"scope": [
				"keyword.type"
			],
			"settings": {
				"foreground": "#674540"
			}
		},
		{
			"name": "Types (<>)",
			"scope": [
				"punctuation.definition.typeparameters"
			],
			"settings": {
				"foreground": "#3e3030"
			}
		},
		{
			"name": "Types (System)",
			"scope": [
				"support.type.System"
			],
			"settings": {
				"foreground": "#3e3030"
			}
		},
		{
			"name": "Modifiers",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#3e3030"
			}
		},
		{
			"name": "Types (declarations)",
			"comment": "Including classes, delegates, enums, interfaces, structs.",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#b86e59"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword.control",
				"keyword.operator.conditional",
				"keyword.query"
			],
			"settings": {
				"foreground": "#ae845f"
			}
		},
		{
			"name": "Keywords (other)",
			"scope": [
				"keyword.other"
			],
			"settings": {
				"foreground": "#78614e"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"meta.preprocessor",
				"meta.preprocessor punctuation",
				"meta.preprocessor punctuation.parenthesis",
				"meta.preprocessor punctuation.squarebracket",
				"meta.preprocessor punctuation.curlybrace",
				"meta.preprocessor keyword"
			],
			"settings": {
				"foreground": "#4b4439"
			}
		},
		{
			"name": "Preprocessor (entities)",
			"scope": [
				"meta.preprocessor entity",
				"meta.preprocessor entity.name.method",
				"meta.preprocessor entity.name.function",
				"meta.preprocessor entity.name.variable",
				"meta.preprocessor variable",
				"meta.preprocessor string"
			],
			"settings": {
				"foreground": "#5d503d"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.other.readwrite",
				"variable.other.object",
				"variable.other.alias",
				"entity.name.variable",
				"variable.other.property",
				"variable.other.constant",
				"variable.parameter",
				"variable.object.property",
				"variable.object.constant",
				"meta.object-literal.key",
				"support.variable"
			],
			"settings": {
				"foreground": "#b9a659"
			}
		},
		{
			"name": "this (local)",
			"scope": [
				"keyword.other.this",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#2CA555",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables (local)",
			"scope": [
				"variable.other.object.property",
				"entity.name.variable.local",
				"keyword.other.base",
				"variable.language.base"
			],
			"settings": {
				"foreground": "#7d735a"
			}
		},
		{
			"name": "Variables (System)",
			"scope": [
				"support.variable.System"
			],
			"settings": {
				"foreground": "#4a4636"
			}
		}
	]
}